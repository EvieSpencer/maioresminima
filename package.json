    "extend-shallow": "^2.0.1",
    "for-own": "^0.1.4",
    "front-matter": "^2.1.2",
    "gulp-format-md": "^0.1.12",
    "matched": "^1.0.2",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "sort-object": "^3.0.2",
    "toml": "^2.3.2",
    "vinyl": "^2.0.2"
