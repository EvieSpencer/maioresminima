  },
  "license": "MIT",
  "files": [
    "gray-matter.d.ts",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "js-yaml": "^3.8.1",
    "kind-of": "^5.0.0",
    "strip-bom-string": "^1.0.0"
  },
